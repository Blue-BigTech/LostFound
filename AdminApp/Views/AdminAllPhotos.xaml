                                                                        </TextBlock>
                                                                    </Viewbox>
                                                                </Grid>
                                                                <Grid Grid.Column="2" >
                                                                    <Viewbox >
                                                                        <TextBlock HorizontalAlignment="Right" Foreground="Black" Width="175" Text="{Binding CreatedDate}" TextWrapping="Wrap">
                                                                        </TextBlock>
                                                                    </Viewbox>
                                                                </Grid>
                                                                <Grid Grid.Column="4" Visibility="{Binding DeleteBtnVis}">
                                                                    <Viewbox >
                                                                        <Button CornerRadius="50" Click="DeletePhotosCommand" CommandParameter="{Binding Code}" Style="{StaticResource ButtonStyle}" Width="50" Height="50" BorderBrush="Transparent" BorderThickness="0" Background="Transparent" Padding="0" Visibility="{Binding GenerateUserBtnVis}">
                                                                            <Image Source="/Assets/delete.png" Width="30"/>
