                                                            </Grid>                                                        </Grid>                                                    </DataTemplate>                                                </ListBox.ItemTemplate>                                            </ListBox>                                        </Grid>
                                    </Grid>
                                </Grid>


                            </Grid>
                        </ScrollViewer>
                    </Grid>

                    <Grid Grid.Row="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height=".1*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height=".1*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width=".2*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="1" Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2.5*"></ColumnDefinition>
                                <ColumnDefinition Width=".8*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width=".4*"></ColumnDefinition>
                                    <ColumnDefinition Width=".0*"></ColumnDefinition>
                                    <ColumnDefinition Width=".22*"></ColumnDefinition>
                                    <ColumnDefinition Width=".0*"></ColumnDefinition>
                                    <ColumnDefinition Width=".4*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <Viewbox>

                                        <Button CornerRadius="50" Command="{Binding Path=PreviousCommand}" IsEnabled="{Binding PreviousButtonEnable}" Style="{StaticResource ButtonStyle}" Width="80" Height="80" Background="Transparent" Padding="0">
                                            <Image HorizontalAlignment="Left" Source="/Assets/Preview.png" Width="80"/>
                                            <Button.Resources>
                                                <ResourceDictionary>
                                                    <ResourceDictionary.ThemeDictionaries>
                                                        <ResourceDictionary x:Key="Light">
                                                            <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Transparent"/>
                                                            <SolidColorBrush x:Key="ButtonForeground" Color="Transparent"/>
                                                        </ResourceDictionary>
                                                    </ResourceDictionary.ThemeDictionaries>
                                                </ResourceDictionary>
                                            </Button.Resources>

                                        </Button>

                                    </Viewbox>
                                </Grid>
                                <Grid x:Name="DisabledbackBtnVis" Grid.Column="0" CornerRadius="50" Width="40" Height="40" Background="#8888" Visibility="{Binding DisabledbackBtnVis}">

                                </Grid>
                                <Grid Grid.Column="2" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height=".1*"></RowDefinition>
                                        <RowDefinition Height=".8*"></RowDefinition>
                                        <RowDefinition Height=".1*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="1" Background="White" CornerRadius="100">
                                        <Viewbox >
                                            <TextBlock FontSize="27" Padding="3" Text="{Binding PageNo}"></TextBlock>
                                        </Viewbox>
                                    </Grid>

                                </Grid>
                                <Grid Grid.Column="4">
                                    <Viewbox>


                                        <Button CornerRadius="50" Command="{Binding Path=NextCommand}" IsEnabled="{Binding NextButtonEnable}" Style="{StaticResource ButtonStyle}" Width="80" Height="80" Background="Transparent" Padding="0">
                                            <Image Source="/Assets/Next.png" Width="80"/>
                                            <Button.Resources>
                                                <ResourceDictionary>
                                                    <ResourceDictionary.ThemeDictionaries>
                                                        <ResourceDictionary x:Key="Light">
                                                            <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Transparent"/>
                                                            <SolidColorBrush x:Key="ButtonForeground" Color="#BEBEC6"/>
                                                        </ResourceDictionary>
                                                    </ResourceDictionary.ThemeDictionaries>
                                                </ResourceDictionary>
                                            </Button.Resources>

                                        </Button>
                                    </Viewbox>

                                

                                </Grid>
                                <Grid x:Name="DisabledBtnVis" Grid.Column="4" CornerRadius="50" Width="40" Height="40" Background="#8888" Visibility="{Binding DisabledBtnVis}">

                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>

                </Grid>

            </Grid>
            <Grid Grid.Column="2" HorizontalAlignment="Right" Visibility="{Binding DeleteBtnProgressRingVis}">
                <Viewbox>
                    <ProgressRing HorizontalAlignment="Stretch" Foreground="#447295" VerticalAlignment="Center" Height="10" IsActive="False"></ProgressRing>
                </Viewbox>
            </Grid>
        </Grid>

        <Grid Grid.RowSpan="4" Grid.ColumnSpan="3" x:Name="ResetPasswordGrid" Visibility="{Binding ResetPopupGridVis}">
            <RelativePanel>
                <Viewbox HorizontalAlignment="Center">
                    <Popup x:Name="ResetPasswordPopup" IsOpen="{Binding PopupOpen}" VerticalOffset="120" HorizontalOffset="230" HorizontalAlignment="Center"
                                   Width="10" Height="320" LightDismissOverlayMode="On">

                        <Grid Background="#447295" CornerRadius="10 10 10 10" HorizontalAlignment="Center" Width="170" Height="70">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".1*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width=".1*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height=".4*"></RowDefinition>
                                <RowDefinition Height="1.1*"></RowDefinition>
                                <!--<RowDefinition Height=".1*"></RowDefinition>
                                <RowDefinition Height="1*"></RowDefinition>-->
                                <RowDefinition Height=".4*"></RowDefinition>
                                <RowDefinition Height=".2*"></RowDefinition>
                                <RowDefinition Height=".8*"></RowDefinition>
                                <RowDefinition Height=".3*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="1" Grid.Column="1">
                                <Viewbox>
                                    <TextBox Text="{Binding NewPassword, Mode=TwoWay}" Style="{StaticResource TextBoxStyle}"
                             PlaceholderText="New Password">
                                        <TextBox.Resources>
                                            <ResourceDictionary>
                                                <ResourceDictionary.ThemeDictionaries>
                                                    <ResourceDictionary x:Key="Light">
                                                        <SolidColorBrush x:Key="TextControlForegroundPointerOver" Color="Black"/>
                                                        <SolidColorBrush x:Key="TextControlBackgroundPointerOver" Color="White"/>
                                                        <SolidColorBrush x:Key="TextControlBackgroundFocused" Color="White"/>
                                                        <SolidColorBrush x:Key="TextControlForegroundFocused" Color="Black"/>
                                                        <SolidColorBrush x:Key="TextControlButtonForeground" Color="Black"/>
                                                        <SolidColorBrush x:Key="TextControlButtonForegroundPointerOver" Color="Black"/>
                                                    </ResourceDictionary>
                                                </ResourceDictionary.ThemeDictionaries>
                                            </ResourceDictionary>
                                        </TextBox.Resources>
                                    </TextBox>
                                </Viewbox>
                            </Grid>

                            <Grid Grid.Row="2" Grid.Column="1" Visibility="{Binding NewPasswordValidationMsg}">
                                <Viewbox VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding PasswordStrength, Mode=TwoWay}" x:Name="NewPasswordValidationMsg" TextAlignment="Left" TextWrapping="Wrap"
                               FontSize="5" FontWeight="Medium" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                                </Viewbox>
                            </Grid>
                            <Grid Grid.Row="4" Grid.Column="1">                                <Grid.ColumnDefinitions>                                    <ColumnDefinition Width="*"></ColumnDefinition>                                    <ColumnDefinition Width="*"></ColumnDefinition>                                    <ColumnDefinition Width=".1*"></ColumnDefinition>                                    <ColumnDefinition Width="*"></ColumnDefinition>                                    <ColumnDefinition Width="*"></ColumnDefinition>                                </Grid.ColumnDefinitions>                                <!--Visibility="{Binding UpdatePasswordGridVis}"-->                                <Grid Grid.Column="1" >                                    <Viewbox>                                        <Button Command="{Binding Path=UpdateUserPasswordCommand}" Style="{StaticResource ButtonStyle}"  Foreground="Black" Background="WhiteSmoke">
                                            <Button.Resources>
                                                <ResourceDictionary>
                                                    <ResourceDictionary.ThemeDictionaries>
                                                        <ResourceDictionary x:Key="Light">
                                                            <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#447295"/>
                                                            <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#447295"/>
                                                            <SolidColorBrush x:Key="ButtonForeground" Color="White"/>
                                                        </ResourceDictionary>
                                                    </ResourceDictionary.ThemeDictionaries>
                                                </ResourceDictionary>
                                            </Button.Resources>
                                            <Viewbox>
                                                <TextBlock Visibility="{Binding UpdatePasswordGridVis}" FontWeight="Medium" 
                        VerticalAlignment="Center"   FontSize="8"  Padding="3 1 5 3" Text="Update"></TextBlock>
                                            </Viewbox>
                                        </Button>                                    </Viewbox>                                </Grid>                                <Grid Grid.Row="1" Grid.Column="1" Visibility="{Binding UpdatePasswordProgressRingVis}" x:Name="UpdatePasswordProgressRingVis">
                                    <Viewbox>
                                        <ProgressRing HorizontalAlignment="Stretch" Foreground="#447295" VerticalAlignment="Center" Height="30" IsActive="True"></ProgressRing>
                                    </Viewbox>
                                </Grid>                                <Grid Grid.Column="3">                                    <Viewbox>                                        <Button Content="Cancel" Style="{StaticResource ButtonStyle}"  Command="{Binding Path=CancelbtnCommand}" Foreground="Black"  Background="WhiteSmoke">                                            <Button.Resources>                                                <ResourceDictionary>                                                    <ResourceDictionary.ThemeDictionaries>                                                        <ResourceDictionary x:Key="Light">                                                            <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>                                                            <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="Gray"/>                                                            <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>                                                            <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Gray"/>                                                            <SolidColorBrush x:Key="ButtonForeground" Color="White"/>                                                        </ResourceDictionary>                                                    </ResourceDictionary.ThemeDictionaries>                                                </ResourceDictionary>                                            </Button.Resources>
                                        </Button>                                    </Viewbox>                                </Grid>                            </Grid>
                        </Grid>
                    </Popup>
                </Viewbox>
            </RelativePanel>
        </Grid>

        <Grid Grid.RowSpan="4" Grid.ColumnSpan="3" x:Name="LogoutPopupGrid">
            <RelativePanel>
                <Viewbox HorizontalAlignment="Center">
                    <Popup x:Name="LogoutPopup" IsOpen="{Binding LogoutPopupOpen}" VerticalOffset="120" HorizontalOffset="230" HorizontalAlignment="Center"
                                   Width="10" Height="320" IsLightDismissEnabled="True" LightDismissOverlayMode="On">

                        <Grid Background="#447295" CornerRadius="10 10 10 10" HorizontalAlignment="Center" Width="170" Height="70">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".1*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width=".1*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height=".1*"></RowDefinition>                                <RowDefinition Height="1*"></RowDefinition>                                <RowDefinition Height=".1*"></RowDefinition>                                <RowDefinition Height=".8*"></RowDefinition>                                <RowDefinition Height=".4*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="1" Grid.Column="1">
                                <Viewbox>
                                    <TextBlock Foreground="White" Text="Do you really want to log out from the software?"></TextBlock>
                                </Viewbox>
                            </Grid>
                            <Grid Grid.Row="3" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width=".1*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="1">
                                    <Viewbox>
                                        <Button FontSize="8" Command="{Binding SureCommand}" Content="Yes" Width="45"  Foreground="Black" Background="WhiteSmoke">
                                            <Button.Resources>
                                                <ResourceDictionary>
                                                    <ResourceDictionary.ThemeDictionaries>
                                                        <ResourceDictionary x:Key="Light">
                                                            <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#447295 "/>
                                                            <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#447295"/>
                                                            <SolidColorBrush x:Key="ButtonForeground" Color="White"/>
                                                        </ResourceDictionary>
                                                    </ResourceDictionary.ThemeDictionaries>
                                                </ResourceDictionary>
                                            </Button.Resources>

                                        </Button>
                                    </Viewbox>
                                </Grid>
                                <Grid Grid.Column="3">
                                    <Viewbox>
                                        <Button FontSize="8" Command="{Binding DontLogoutCommand}" Content="No" Width="45" Foreground="Black"  Background="WhiteSmoke">
                                            <Button.Resources>
                                                <ResourceDictionary>
                                                    <ResourceDictionary.ThemeDictionaries>
                                                        <ResourceDictionary x:Key="Light">
                                                            <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="Gray"/>
                                                            <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                                            <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Gray"/>
                                                            <SolidColorBrush x:Key="ButtonForeground" Color="White"/>
                                                        </ResourceDictionary>
                                                    </ResourceDictionary.ThemeDictionaries>
                                                </ResourceDictionary>
                                            </Button.Resources>

                                        </Button>
                                    </Viewbox>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Popup>
                </Viewbox>
            </RelativePanel>
        </Grid>

        <Grid x:Name="LoadingBackgroundGrid" Grid.RowSpan="4" Grid.ColumnSpan="3" Background="#8888" Visibility="{Binding LoadingBackgroundGridVis}">

        </Grid>

        <Grid Grid.RowSpan="4" Grid.ColumnSpan="3">
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="5*"></RowDefinition>
                <RowDefinition Height="1.5*"></RowDefinition>
            </Grid.RowDefinitions>

            <Grid Grid.Column="0"></Grid>
            <Grid x:Name="NotificationPopupGrid" Grid.Row="2" HorizontalAlignment="Right" Visibility="{Binding NotificationPopupGridVis}">


                <Grid >
                    <RelativePanel>
                        <Viewbox>
                            <Popup x:Name="NotificationPopup" IsOpen="{Binding NotificationPopup}" RelativePanel.AlignRightWithPanel="True"
                                   Width="170" Height="80" IsLightDismissEnabled="True" LightDismissOverlayMode="On">
                                <Popup.ChildTransitions>
                                    <TransitionCollection>
                                        <PaneThemeTransition Edge="Right"/>
                                    </TransitionCollection>
                                </Popup.ChildTransitions>
                                <Grid Background="#1A1C22" CornerRadius="2 2 2 2" HorizontalAlignment="Center" Height="35" Width="160">
                                    <Grid.ColumnDefinitions>
                                    </Grid.ColumnDefinitions>
                                    <Grid Margin="0 0 0 0">
                                        <Viewbox VerticalAlignment="Center" HorizontalAlignment="Center" Width="Auto" Height="11">
                                            <TextBlock Text="{Binding NotificationMessage, Mode=TwoWay}" x:Name="NotificationMessage" TextAlignment="Center" Padding="0 0 0 0" 
                                                       FontSize="6" FontWeight="Medium" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                                        </Viewbox>
                                    </Grid>
                                </Grid>
                            </Popup>
                        </Viewbox>
                    </RelativePanel>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</Page>
